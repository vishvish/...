{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "uptime": {
      "before": "⇑  "
    },
    "date": {
      "before": "⌚  "
    }
  },
  "segments": {
    "right": [
      {
        "name": "weather"
      },
      {
        "name": "battery",
        "args": {
          "gamify": true
        }
      },
      {
        "name": "date",
        "args": {
          "format": "%m-%d",
          "istime": true
        }
      },
      {
        "name": "fuzzy_time"
      },
      {
        "name": "hostname"
      }
    ]
  }
}
